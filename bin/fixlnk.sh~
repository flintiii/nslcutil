!#/bin/bash
pwd
read -p "you better be in the right directory...2git" ans
#
rm -rf example.tab
rm -rf nslcobjects.py
rm -rf example.xlsx
rm -rf example.csv
rm -rf nslcutil.py
#
echo "relinking..."
#
ln example.tab  /example.tab
ln nslcobjects.py  /bin/nslcobjects.py
ln example.xlsx  /example.xlsx
ln example.csv  /example.csv
ln nslcutil.py  /bin/nslcutil.py

